<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

  <appSettings>
    <add key="MongoDB.Server" value="mongodb://localhost:27017"/>
    <add key="VBinBootStrapperType" value="VBin.MongoBootstrapper,vbin"/>
    <add key="VBinDatabase" value="vbin"/>
  </appSettings>

  <system.net>
    <defaultProxy>
      <proxy usesystemdefault="True" bypassonlocal="False"/>
    </defaultProxy>
  </system.net>
</configuration>
