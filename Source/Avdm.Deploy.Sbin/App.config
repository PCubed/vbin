<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <appSettings>
    <add key="MongoDB.Server" value="mongodb://localhost:27017" />
    <add key="SbinBootStrapperType" value="Avdm.Deploy.Sbin.MongoBootstrapper,sbin" />
  </appSettings>

  <system.net>
    <defaultProxy>
      <proxy usesystemdefault="True" bypassonlocal="False"/>
    </defaultProxy>
  </system.net>
</configuration>